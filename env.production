# 本番環境設定
NODE_ENV=production

# データベース設定
DATABASE_URL=postgresql://postgres:your_secure_password@localhost:5432/railway_maintenance
DB_HOST=localhost
DB_PORT=5432
DB_NAME=railway_maintenance
DB_USER=postgres
DB_PASSWORD=your_secure_password

# アプリケーション設定
NEXT_PUBLIC_APP_URL=https://your-domain.com
PORT=3000

# セキュリティ設定
NEXTAUTH_SECRET=your_nextauth_secret_key_here
NEXTAUTH_URL=https://your-domain.com

# ログ設定
LOG_LEVEL=info

# パフォーマンス設定
NEXT_TELEMETRY_DISABLED=1 