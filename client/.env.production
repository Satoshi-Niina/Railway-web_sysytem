# ===================================
# Client - 本番環境用設定 (.env.production)
# ===================================

# 環境設定
NODE_ENV=production

# アプリケーション設定
# GitHub Secrets 等から注入してください
NEXT_PUBLIC_APP_URL=
NEXT_PUBLIC_API_URL=

# ログレベル
NEXT_PUBLIC_LOG_LEVEL=error

# 開発モード（本番環境では必ずfalse）
NEXT_PUBLIC_DEV_MODE=false

# セキュリティ設定
NEXT_PUBLIC_FORCE_HTTPS=true
