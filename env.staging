# ステージング環境設定
NODE_ENV=staging

# データベース設定
DATABASE_URL=postgresql://postgres:staging_password@localhost:5433/railway_maintenance_staging
DB_HOST=localhost
DB_PORT=5433
DB_NAME=railway_maintenance_staging
DB_USER=postgres
